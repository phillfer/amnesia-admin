{
  "name": "react-universally",
  "version": "13.0.0",
  "description": "A starter kit for universal react applications.",
  "main": "build/server/index.js",
  "scripts": {
    "analyze:client": "babel-node internal/scripts/analyze --client",
    "analyze:server": "babel-node internal/scripts/analyze --server",
    "build": "babel-node internal/scripts/build --optimize",
    "build:dev": "babel-node internal/scripts/build",
    "clean": "cross-env babel-node internal/scripts/clean",
    "deploy": "babel-node internal/scripts/deploy",
    "mock": "babel-node mockserver",
    "develop": "cross-env DEPLOYMENT=development babel-node internal/development",
    "lint": "eslint client server shared config internal",
    "precommit": "lint-staged",
    "prepush": "jest",
    "start": "cross-env NODE_ENV=production node build/server",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-import-meta",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ],
      "@babel/plugin-proposal-json-strings",
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "shared/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/internal/jest/setupFile.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|internal|node_modules|flow-typed|public)/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internal/jest/assetMock.js",
      "\\.(css|scss)$": "<rootDir>/internal/jest/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ctrlplusb/react-universally.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "starter kit",
    "universal",
    "javascript",
    "express",
    "webpack"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ctrlplusb/react-universally/issues"
  },
  "homepage": "https://github.com/ctrlplusb/react-universally#readme",
  "dependencies": {
    "apisauce": "0.16.0",
    "app-root-dir": "1.0.2",
    "autoprefixer": "9.1.5",
    "axios": "0.18.0",
    "babel-plugin-react-css-modules": "3.4.2",
    "body-parser": "1.18.3",
    "chalk": "2.4.1",
    "classnames": "2.2.6",
    "compression": "1.7.3",
    "cors": "2.8.4",
    "cross-env": "5.2.0",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "gsap": "2.0.2",
    "helmet": "3.13.0",
    "history": "4.7.2",
    "hpp": "0.2.2",
    "mobile-detect": "1.4.3",
    "modernizr": "3.6.0",
    "normalize.css": "8.0.0",
    "offline-plugin": "5.0.5",
    "pretty-error": "2.1.1",
    "prop-types": "15.6.2",
    "raf": "3.4.0",
    "re-reselect": "2.1.0",
    "react": "16.5.2",
    "react-async-bootstrapper": "2.1.1",
    "react-async-component": "2.0.0",
    "react-dom": "16.5.2",
    "react-form": "3.5.6",
    "react-google-tag-manager": "2.2.1",
    "react-helmet": "5.2.0",
    "react-hot-loader": "4.3.8",
    "react-jobs": "1.0.1",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-select": "2.0.0",
    "react-slick": "0.23.1",
    "react-text-mask": "5.4.3",
    "react-tree-walker": "4.3.0",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "reselect": "3.0.1",
    "serialize-javascript": "1.5.0",
    "source-map-support": "0.5.9",
    "store": "2.0.12",
    "uglifyjs-webpack-plugin": "2.0.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.1.0",
    "@babel/core": "7.1.0",
    "@babel/node": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-syntax-import-meta": "7.0.0",
    "@babel/plugin-transform-react-constant-elements": "7.0.0",
    "@babel/plugin-transform-react-inline-elements": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "assets-webpack-plugin": "3.9.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-template": "6.26.0",
    "chokidar": "2.0.4",
    "css-loader": "1.0.0",
    "detect-port-alt": "1.1.6",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "glob": "7.1.3",
    "happypack": "5.0.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "istanbul-api": "2.0.6",
    "istanbul-reports": "2.0.0",
    "jest": "23.6.0",
    "lint-staged": "7.2.2",
    "md5": "2.2.1",
    "mini-css-extract-plugin": "0.4.3",
    "modernizr-loader": "1.0.1",
    "node-notifier": "5.2.1",
    "node-sass": "4.9.3",
    "normalize-scss": "7.0.1",
    "postcss-loader": "3.0.0",
    "postcss-scss": "2.0.0",
    "prettier": "1.14.2",
    "react-dev-utils": "5.0.2",
    "react-test-renderer": "16.5.2",
    "regenerator-runtime": "0.12.1",
    "request": "2.88.0",
    "rimraf": "2.6.2",
    "sass-loader": "7.1.0",
    "sass-resources-loader": "1.3.3",
    "semver": "5.5.1",
    "style-loader": "0.23.0",
    "webpack": "4.19.1",
    "webpack-bundle-analyzer": "3.0.2",
    "webpack-dev-middleware": "3.3.0",
    "webpack-hot-middleware": "2.24.0",
    "webpack-node-externals": "1.7.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "prettier"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "linebreak-style": 0,
      "no-underscore-dangle": 0,
      "no-return-assign": 0,
      "prefer-destructuring": 0,
      "import/prefer-default-export": 0,
      "react/destructuring-assignment": 0,
      "react/jsx-filename-extension": 0,
      "react/sort-comp": 0,
      "react/jsx-one-expression-per-line": 0,
      "react/require-default-props": 0,
      "react/prefer-stateless-function": 0
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "build/"
  ],
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
